<%= render 'navbar' %>
<div class="container">
  <div class="row">
    <% @issues.each do |issue| %>
      <div class="col-xs-12 col-sm-12 col-md-6">
        <%= render 'card', issue: issue %>
      </div>
    <% end %>
  </div>
  <div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>">
  </div>

</div>
